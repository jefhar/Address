{
  "name": "jefharr/address",
  "description": "An encapsulation of an address.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jeff Harris",
      "email": "jeff@jeffharris.us"
    }
  ],
  "autoload": {
    "psr-4": {
      "JefHar\\Address\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JefHar\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "require": {
    "ext-dom": "^20031129",
    "ext-json": "^1.6.0",
    "ext-libxml": "^7.2.0",
    "ext-mbstring": "^7.2.0",
    "ext-openssl": "^7.2.0",
    "ext-pcre": "^7.2.0",
    "ext-Phar": "^2.0.2",
    "ext-SimpleXML": "^7.2.0",
    "ext-tokenizer": "^7.2.0",
    "ext-xml": "^7.2.0",
    "ext-xmlwriter": "^7.2.0",
    "php": "^7.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4",
    "sensiolabs/security-checker": "^5.0",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "scripts": {
    "phpcs": [
      "rm -rf tests/coverage",
      "vendor/bin/phpcs --standard=PSR12 -sn src tests"
    ],
    "phpcbf": [
      "rm -rf tests/coverage",
      "vendor/bin/phpcbf --standard=PSR12 src tests"
    ],
    "test": "vendor/bin/phpunit"
  }
}
